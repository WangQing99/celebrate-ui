{
  "private": true,
  "packageManager": "pnpm@7.12.2",
  "workspaces": [
    "packages/*",
    "playground",
    "docs"
  ],
  "description": "Celebrate UI Monorepo.",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm -C playground dev",
    "build": "pnpm run -C internal/build start",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "stub": "pnpm run -r --parallel stub"
  },
  "dependencies": {
    "@yarnpkg/core": "^4.0.0-rc.14"
  },
  "devDependencies": {
    "@celebrate-ui/base-components": "workspace:*",
    "@celebrate-ui/cli": "workspace:*",
    "@celebrate-ui/constants": "workspace:*",
    "@celebrate-ui/eslint-config": "workspace:*",
    "@celebrate-ui/node-utils": "workspace:*",
    "@celebrate-ui/theme-chalk": "workspace:*",
    "@celebrate-ui/tsconfig": "workspace:*",
    "@celebrate-ui/utils": "workspace:*",
    "@esbuild-kit/cjs-loader": "^2.2.1",
    "@pnpm/find-workspace-packages": "^4.0.16",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "^8.4.0",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/jsdom": "^16.2.14",
    "@types/node": "*",
    "@types/sass": "^1.43.1",
    "@vitejs/plugin-vue": "3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/runtime-core": "^3.2.38",
    "@vue/tsconfig": "^0.1.3",
    "@wang-qing/eslint-config": "^1.3.0",
    "c8": "^7.11.3",
    "consola": "^2.15.3",
    "element-plus": "^2.2.15",
    "eslint": "8.22.0",
    "fast-glob": "^3.2.11",
    "jsdom": "16.4.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "ts-morph": "^14.0.0",
    "tsx": "^3.6.0",
    "type-fest": "^2.14.0",
    "typescript": "^4.7.4",
    "unplugin-vue-define-options": "0.6.1",
    "vitest": "^0.22.1",
    "vue": "^3.2.37"
  },
  "engines": {
    "node": ">= 16"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "react",
        "react-dom"
      ]
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}

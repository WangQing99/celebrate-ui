{
  "name": "@celebrate-ui/node-utils",
  "version": "1.0.0",
  "description": "ESLint config for Celebrate UI.",
  "keywords": [
    "celebrate-ui",
    "celebrate",
    "component library",
    "ui framework",
    "ui",
    "vue",
    "eslint-config"
  ],
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "unbuild",
    "dev": "pnpm run stub",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "@celebrate-ui/node-constants": "workspace:*",
    "consola": "^2.15.3",
    "typescript": "^4.6.3"
  }
}
